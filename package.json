{
  "name": "onecx-service-auth-ui",
  "private": true,
  "version": "0.0.0",
  "license": "Apache-2.0",
  "contributors": [
    "OneCX Development Team <onecx_dev@1000kit.org>"
  ],
  "config": {
    "openapiYaml": "libs/custom-auth/src/assets/api/openapi-bff.yaml",
    "openapiOutput": "libs/custom-auth/src/lib/shared/generated"
  },
  "scripts": {
    "build": "nx run custom-auth:build --verbose",
    "check": "nx lint --cache=true && npx prettier -c --cache .",
    "clean": "npm cache clean --force && rm -rf *.log dist reports .nx .angular .eslintcache ./node_modules/.cache/prettier/.prettier-cache",
    "lint": "nx lint --cache=true",
    "format": "nx lint --fix",
    "prepare": "husky || true",
    "start": "nx run custom-auth:build --verbose && npx webpack serve",
    "test": "nx test",
    "test:ci": "nx test --watch=false --browsers=ChromeHeadless --coverage",
    "sonar": "npx sonar-scanner",
    "apigen-cleanup": "rm -rf $npm_package_config_openapiOutput",
    "apigen-format": "npx prettier $npm_package_config_openapiYaml --write && npx prettier $npm_package_config_openapiOutput/**/* --write",
    "apigen-generate": "openapi-generator-cli generate -i $npm_package_config_openapiYaml -g typescript-fetch -c apigen.yaml -o $npm_package_config_openapiOutput --type-mappings=AnyType=object,set=Array",
    "apigen": "npm run apigen-cleanup && npm run apigen-generate && npm run apigen-format"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jestSonar": {
    "reportPath": "reports"
  },
  "nx": {
    "includedScripts": []
  },
  "dependencies": {
    "@ngx-translate/core": "^15.0.0",
    "@nx/angular": "19.8.2",
    "@nx/webpack": "^17.3.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nx/jest": "19.8.2",
    "@nx/js": "19.8.2",
    "@nx/workspace": "19.8.2",
    "@nx/eslint": "19.8.2",
    "@nx/eslint-plugin": "19.8.2",
    "@angular-eslint/builder": "^18.4.3",
    "@angular-eslint/eslint-plugin": "^18.4.3",
    "@angular-eslint/eslint-plugin-template": "^18.4.3",
    "@angular-eslint/schematics": "^18.4.3",
    "@angular-eslint/template-parser": "^18.4.3",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@schematics/angular": "^18.2.12",
    "@swc-node/register": "~1.8.0",
    "@swc/core": "~1.3.85",
    "@swc/helpers": "~0.5.2",
    "@types/jest": "^29.4.0",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.4.1",
    "jest-marbles": "3.1.1",
    "jest-preset-angular": "14.2.4",
    "jest-sonar": "^0.2.16",
    "jest-sonar-reporter": "^2.0.0",
    "nx": "19.8.2",
    "prettier": "^3.5.3",
    "sonar-scanner": "^3.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.5.4",
    "webpack-cli": "^5.1.4"
  }
}
